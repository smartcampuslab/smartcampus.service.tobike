<?xml version="1.0" encoding="UTF-8"?>
<dataflow xmlns="http://smart.sayservice.it/schema/dataflow-schema"	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://smart.sayservice.it/schema/dataflow-schema http://smart.sayservice.it/schema/dataflow-schema-1.7.xsd">

	<input name="user" type="String" />
	<input name="password" type="String" />
	<input name="code" type="String" />

	<script outputVar="output">
		<className>eu.trentorise.smartcampus.service.tobike.scripts.TobikeScript</className>
		<method>getStazioni</method>
		<variables>
			<value>user</value>
			<value>password</value>
			<value>code</value>
		</variables>
	</script>


</dataflow>
