	<input name="user" type="String" />
	<input name="password" type="String" />
	<input name="code" type="String" />

	<script outputVar="output">
		<className>eu.trentorise.smartcampus.service.tobike.scripts.TobikeScript</className>
		<method>getStazioni</method>
		<variables>
			<value>user</value>
			<value>password</value>
			<value>code</value>
		</variables>
	</script>


</dataflow>
